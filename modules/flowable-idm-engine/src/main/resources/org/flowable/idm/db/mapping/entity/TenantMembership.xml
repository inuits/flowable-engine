<?xml version="1.0" encoding="UTF-8" ?> 

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
  
<mapper namespace="org.flowable.idm.engine.impl.persistence.entity.TenantMembershipEntityImpl">
  
  <!-- TENANT_MEMBERSHIP INSERT -->

  <insert id="insertTenantMembership" parameterType="map">
    insert into ${prefix}ACT_ID_TENANT_MEMBERSHIP (USER_ID_, TENANT_ID_)
    values (
      #{userId ,jdbcType=VARCHAR},
      #{tenantId ,jdbcType=VARCHAR}
    )
  </insert>

  <insert id="bulkInsertTenantMembership" parameterType="java.util.List">
    INSERT INTO ${prefix}ACT_ID_TENANT_MEMBERSHIP (USER_ID_, TENANT_ID_)
    VALUES 
    <foreach collection="list" item="membership" index="index" separator=","> 
      (#{membership.userId ,jdbcType=VARCHAR},
       #{membership.tenantId ,jdbcType=VARCHAR})
    </foreach>
  </insert>

  <insert id="bulkInsertTenantMembership" databaseId="oracle" parameterType="java.util.List">
    INSERT ALL 
    <foreach collection="list" item="membership" index="index">
      INTO ${prefix}ACT_ID_TENANT_MEMBERSHIP (USER_ID_, TENANT_ID_) VALUES  
        (#{membership.userId ,jdbcType=VARCHAR},
         #{membership.tenantId ,jdbcType=VARCHAR})
    </foreach>
    SELECT * FROM dual
  </insert>

  <!-- TENANT_MEMBERSHIP UPDATE -->
  
  <!-- TENANT_MEMBERSHIP DELETE -->

  <delete id="deleteTenantMembershipsByUserId" parameterType="string">
    delete from ${prefix}ACT_ID_TENANT_MEMBERSHIP 
    where USER_ID_ = #{userId} 
  </delete>

  <delete id="deleteTenantMembership" parameterType="map">
    delete from ${prefix}ACT_ID_TENANT_MEMBERSHIP 
    where USER_ID_ = #{userId}
      and TENANT_ID_ = #{tenantId} 
  </delete>

  <!-- TENANT_MEMBERSHIP RESULTMAP -->
  
  <!-- TENANT_MEMBERSHIP DELETE -->

  <delete id="deleteTenantMembershipsByTenantId" parameterType="string">
    delete from ${prefix}ACT_ID_TENANT_MEMBERSHIP 
    where TENANT_ID_ = #{tenantId} 
  </delete>
  
</mapper>